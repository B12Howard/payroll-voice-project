{
  "name": "extract-dates-fn",
  "type": "module",
  "scripts": {
    "start": "functions-framework --target=extractDates --port=8080",
    "start:crud": "functions-framework --target=crud --port=8081",
    "dev": "nodemon --exec \"functions-framework --target=extractDates --port=8080\" --watch . --ext js,json",
    "dev:crud": "nodemon --exec \"functions-framework --target=crud --port=8081\" --watch . --ext js,json",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:crud\""
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^4.0.0",
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2",
    "openai": "^6.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "concurrently": "^8.2.2"
  }
}
